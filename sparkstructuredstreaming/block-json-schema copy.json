                    "$id": "#/properties/header/properties/rent_paid",
                    "type": "string"
                },
                "signature": {
                    "$id": "#/properties/header/properties/signature",
                    "type": "string"
                },
                "timestamp": {
                    "$id": "#/properties/header/properties/timestamp",
                    "type": "integer"
                },
                "timestamp_nanosec": {
                    "$id": "#/properties/header/properties/timestamp_nanosec",
