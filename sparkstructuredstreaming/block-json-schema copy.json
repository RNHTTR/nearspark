                "last_final_block": {
                    "$id": "#/properties/header/properties/last_final_block",
                    "type": "string"
                },
                "latest_protocol_version": {
                    "$id": "#/properties/header/properties/latest_protocol_version",
                    "type": "integer"
                },
                "next_bp_hash": {
                    "$id": "#/properties/header/properties/next_bp_hash",
                    "type": "string"
                },
                "next_epoch_id": {
                    "$id": "#/properties/header/properties/next_epoch_id",
                    "type": "string"
                },
                "outcome_root": {
                    "$id": "#/properties/header/properties/outcome_root",
                    "type": "string"
                },
                "prev_hash": {
                    "$id": "#/properties/header/properties/prev_hash",
                    "type": "string"
                },
                "prev_height": {
                    "$id": "#/properties/header/properties/prev_height",
                    "type": "integer"
                },
