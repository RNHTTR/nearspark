                    "$id": "#/properties/header/properties/validator_proposals",
                    "type": "array",
                    "additionalItems": true,
                    "items": {
                        "$id": "#/properties/header/properties/validator_proposals/items"
                    }
                },
                "validator_reward": {
                    "$id": "#/properties/header/properties/validator_reward",
                    "type": "string"
                }
