                            "gas_limit",
                            "gas_used",
                            "height_created",
                            "height_included",
                            "outcome_root",
                            "outgoing_receipts_root",
                            "prev_block_hash",
                            "prev_state_root",
                            "rent_paid",
                            "shard_id",
                            "signature",
                            "tx_root",
                            "validator_proposals",
                            "validator_reward"
                        ],
                        "properties": {
                            "balance_burnt": {
                                "$id": "#/properties/chunks/items/anyOf/0/properties/balance_burnt",
