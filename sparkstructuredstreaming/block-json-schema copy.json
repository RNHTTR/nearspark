            "type": "array",
            "additionalItems": true,
            "items": {
                "$id": "#/properties/chunks/items",
                "anyOf": [
                    {
                        "$id": "#/properties/chunks/items/anyOf/0",
                        "type": "object",
                        "required": [
                            "balance_burnt",
                            "chunk_hash",
                            "encoded_length",
